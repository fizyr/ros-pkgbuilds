post_install() {
	echo 'You must have the CodeMeter Runtime server running to use'
	echo 'any executable linking against any of the ensenso-sdk libraries.'
	echo ''
	echo 'You also have to import the ensenso-sdk license:'
	echo '  cmu --import -f /usr/share/ensenso-sdk/license/EnsensoProtectionOnly.WibuCmRau'
}

post_upgrade() {
	post_install
}
